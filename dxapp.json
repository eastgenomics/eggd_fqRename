{
  "name": "fqRename_v1.0.0",
  "title": "FastQ Renaming Applet",
  "summary": "Renames FastQ files to make them usable downstream",
  "dxapi": "1.0.0",
  "properties": {
    "githubRelease": "v1.0.0"
  },
  "inputSpec": [
    {
      "name": "fastqs",
      "class": "array:file",
      "optional": false,
      "patterns": [
        "*fq.gz"
      ],
      "help": "FastQ files that need renaming"
    }
  ],
  "outputSpec": [
    {
      "name": "renamed_fastqs",
      "label": "array:file",
      "class": "array:file",
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 1
      }
    },
    "interpreter": "bash",
    "file": "src/fqRename.sh",
    "distribution": "Ubuntu",
    "release": "16.04",
    "version": "1"
  },
  "access": {
    "project": "CONTRIBUTE"
  },
  "regionalOptions": {
    "aws:eu-central-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd1_v2_x2"
        }
      }
    }
  }
}
